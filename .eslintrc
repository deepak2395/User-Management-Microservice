{
  "extends": ["standard", "prettier", "prettier-standard"],
  "plugins": ["prettier", "standard"],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2
  }
}